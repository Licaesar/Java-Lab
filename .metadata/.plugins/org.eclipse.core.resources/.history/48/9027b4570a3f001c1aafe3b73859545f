package entities;

public class Employee {
	
	public String name;
	public double grossSalary;
	public double tax;
	
	public double netSalaty() {
		return grossSalary - tax;
	}
	
	public void IncraseSalaty() {
		
	}
	
	public String toString(double percentage) {
		return "Employee: " + name + ", $" + netSalaty();
	}

}
